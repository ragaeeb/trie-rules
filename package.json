    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "vitest": "^2.0.5"
