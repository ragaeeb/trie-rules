{
  "name": "trie-rules",
  "version": "0.1.0",
  "description": "Performs fast search and replace on a text based on a set of rules.",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragaeeb/trie-rules.git"
  },
