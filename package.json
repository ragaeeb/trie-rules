{
  "name": "trie-rules",
  "version": "0.1.0",
  "description": "Performs fast search and replace on a text based on a set of rules.",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragaeeb/trie-rules.git"
  },
  "files": [
    "dist/main.js",
    "dist/main.js.map",
    "dist/*.d.ts"
  ],
  "private": false,
  "source": "src/index.js",
  "type": "module",
  "jest": {
    "transform": {}
  },
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build && cp src/trie.d.ts dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "search",
    "replace",
    "trie"
  ],
  "author": "Ragaeeb Haq",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "30.0.0-alpha.1",
    "@semantic-release/changelog": "6.0.3",
    "eslint": "8.53.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "30.0.0-alpha.1",
    "parcel": "2.10.3",
    "prettier": "3.0.3",
    "semantic-release": "22.0.7"
  }
